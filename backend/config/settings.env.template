# Backend Environment Variables Template
# Copy this file to settings.env and fill in your actual values

# Database
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Redis
REDIS_URL=redis://host:6379/0

# OpenAI
OPENAI_API_KEY=sk-your-key-here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Tavily
TAVILY_API_KEY=tvly-your-key-here

# LangSmith (Optional)
LANGSMITH_API_KEY=ls__your-key-here
LANGSMITH_PROJECT=python-research-portal

# Honeycomb (Optional)
HONEYCOMB_API_KEY=your-key-here

# Application
APP_ENV=production
LOG_LEVEL=INFO
CORS_ORIGINS=["https://your-frontend-url.streamlit.app"]

# Quality Settings
ENABLE_TECHNICAL_COMPILER=true
COMPILER_QUALITY_THRESHOLD=95.0
QUALITY_THRESHOLD=95.0
TEMPERATURE=0.3
COMPILER_TEMPERATURE=0.4



